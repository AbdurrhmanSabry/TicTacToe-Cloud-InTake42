    <children>
      <Button fx:id="accept" layoutX="75.0" layoutY="126.0" onAction="#handleAcceptAction" prefHeight="40.0" prefWidth="160.0" text="Accept">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Button fx:id="reject" layoutX="276.0" layoutY="126.0" onAction="#handleRejectAction" prefHeight="40.0" prefWidth="160.0" text="Reject">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Label layoutX="60.0" layoutY="31.0" prefHeight="40.0" prefWidth="408.0" stylesheets="@style.css" text="Player X send Invitation to you" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font></Label>
      <ImageView fitHeight="28.0" fitWidth="35.0" layoutX="86.0" layoutY="132.0" pickOnBounds="true" preserveRatio="true" /> 
      <ImageView fitHeight="30.0" fitWidth="31.0" layoutX="276.0" layoutY="131.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
